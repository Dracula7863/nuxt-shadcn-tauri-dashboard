{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Nuxt Shadcn Dashboard",
  "version": "0.2.6",
  "identifier": "com.dianprata.dashboard",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm generate",
    "frontendDist": "../.output/public",
    "devUrl": "http://localhost:3000"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool",
    "fileAssociations": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "app": {
    "windows": [
      {
        "title": "Nuxt Shadcn Dashboard",
        "width": 1379,
        "height": 776,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "url": "index.html"
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": []
      }
    }
  },
  "plugins": {}
}
